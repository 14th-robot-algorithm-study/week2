#include <iostream>
using namespace std;
int N;
int num[10];
char cal[3] = { '*', '+', '-' };
char path[10];

void func(int lev, int result) {
	if (lev == N-1) {
		if (result % 101 == 0 && result != 0) {
			cout << num[0];
			for (int i = 0; i < N - 1; i++) {
				cout << path[i] << num[i + 1];
			}
			cout << "\n";
		}
		return;

	}

	for (int i = 0; i < 3; i++) {
		path[lev] = cal[i];
		if (i == 0) func(lev + 1, result * num[lev + 1]);
		else if (i == 1) func(lev + 1, result + num[lev + 1]);
		else func(lev + 1, result - num[lev + 1]);
	}
}

int main() {
	cin >> N;
	for (int i = 0; i < N; i++) {
		cin >> num[i];
	}

	func(0,num[0]);
}
